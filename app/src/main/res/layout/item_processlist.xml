<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="wrap_content">
  <ImageView
      android:layout_width="40dp"
      android:layout_height="40dp"
      android:id="@+id/iv_processlist_icon"
      android:src="@drawable/app"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tv_processlist_appname"
        android:text="myapp1"
        android:textSize="15sp"
        android:layout_marginTop="10dp"
        android:layout_toRightOf="@id/iv_processlist_icon" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tv_processlist_ram"
        android:text="sdcard"
        android:textSize="10sp"
        android:layout_toRightOf="@id/iv_processlist_icon"
        android:layout_below="@+id/tv_processlist_appname"/>
    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/cb_processlist_check"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        android:focusable="false"
        android:clickable="false"
         />
<!-- android:enable="false"-->
</RelativeLayout>